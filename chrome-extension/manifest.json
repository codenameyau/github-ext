{
  "manifest_version": 2,
  "name": "GitHub Extensions",
  "version": "0.0.0",
  "description": "GitHub Chrome extension.",
  "icons": {
    "19": "assets/github-19.png",
    "38": "assets/github-38.png",
    "64": "assets/github-64.png",
    "128": "assets/github-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "assets/github-19.png",
      "38": "assets/github-38.png",
      "64": "assets/github-64.png",
      "128": "assets/github-128.png"
    },
    "default_title": "GitHub Extensions",
    "default_popup": "popup/popup.html"
  },
  "permissions": [
    "storage"
  ]
}
